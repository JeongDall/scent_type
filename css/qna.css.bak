#qna{
  display: none;
}

.qBox{
  background-color: white;
  text-align: center;
  font-size: 15px;
  width: 80%;
  padding: 10px 20px 10px 20px;
}

.answerList{
  background-color: white;
  color: black;
  border-radius: 20px;
  display: block;
  width: 70%;
  border: 0px;
  font-size: 10px;
}

.answerList:hover, .answerList:focus{
  background-color: white;
  color: black;
}

.status{
  width: 80%;
  height: 8px;
  background-color: white;
  border-radius: 20px;
}

.statusBar{
  height: 100%;
  background-color: #007087;
  border-radius: 20px;
}

.pageNumber{
  color: white;
  text-align: center;
}